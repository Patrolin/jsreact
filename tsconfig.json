{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "jsreact": ["src/jsreact/jsreact.ts"]
    },
    "allowImportingTsExtensions": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "types": ["node", "@types/react"],
    "noEmit": true,
    "strictNullChecks": true,
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": ["node_modules/vite/client.d.ts", "src/**/*"]
}
